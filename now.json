{
  "alias": ["zeit.co/tobi/oeu-price-calc"],
  "version": 2,
  "name": "oeu-price-calc",
  "builds": [
    { "src": "package.json", "use": "@now/build" }
  ],
  "routes": [
    {"src": "^/js/(.*)", "dest": "/js/$1"},
    {"src": "^/css/(.*)", "dest": "/css/$1"},
    {"src": "^/img/(.*)", "dest": "/img/$1"},
    {"src": ".*", "dest": "/index.html"}
  ]
}
